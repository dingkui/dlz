<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dlz.framework.db.dao.IDaoOperator">
	<select id="getSeq" resultType="long">
		select ${seqName}.NEXTVAL from DUAL
	</select>
	<select id="getWithTimeSeq" resultType="long">
		SELECT TO_NUMBER(TO_CHAR(sysdate, 'YYYYMMDDHH24MISS')) * 100000 + ${seqName}.NEXTVAL FROM DUAL
	</select>
	<select id="getList" resultType="com.dlz.framework.db.modal.ResultMap" parameterType="com.dlz.framework.db.modal.BaseParaMap">
		${sql_page}
	</select>
	<select id="getPageCnt" resultType="int" parameterType="com.dlz.framework.db.modal.BaseParaMap">
		${sql_cnt}
	</select>
	<update id="updateSql" parameterType="com.dlz.framework.db.modal.BaseParaMap">
		${sqlRun}
	</update>
</mapper>